@import 'nib'
@import '../variables'

.feedbackIcon
	height 100%
	width 100%
	opacity 0.5
	-webkit-user-select none
.no-touch .feedbackIcon:hover
	opacity 1
.feedbackIconBorder
	fill midnight
.feedbackIconPen
	fill midnight
.feedbackIconTopLine
	fill midnight
.feedbackIconMidTopLine
	fill midnight
.feedbackIconMidBottomLine
	fill midnight
.feedbackIconBottomLine
	fill midnight