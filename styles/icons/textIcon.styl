@import 'nib'
@import '../variables'

.textIconWrapper
	transition transform normalSpeed
.textIcon
	height inherit
	width inherit
	-webkit-user-select none
.textIconTopLine
	fill midnight90
.textIconMidTopLine
	fill midnight90
.textIconMiddleLine
	fill midnight90
.textIconMidBottomLine
	fill midnight90
.textIconBottomLine
	fill midnight90
.textIconPeriod
	fill midnight90